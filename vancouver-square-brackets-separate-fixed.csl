<?xml version="1.0" encoding="utf-8"?>
<style xmlns="http://purl.org/net/xbiblio/csl" version="1.0"
       class="in-text" default-locale="en-US"
       demote-non-dropping-particle="never">
  <info>
    <title>Vancouver Square Brackets Separate Fixed</title>
    <id>http://www.zotero.org/styles/vancouver-square-brackets-separate-fixed</id>
    <link href="http://www.zotero.org/styles/vancouver-square-brackets-separate-fixed" rel="self"/>
    <link href="http://www.zotero.org/styles/vancouver" rel="template"/>
    <author>
      <name>Modified by ChatGPT</name>
    </author>
    <category citation-format="numeric"/>
    <category field="medicine"/>
    <updated>2025-09-26T00:00:00+00:00</updated>
  </info>

  <macro name="author">
    <names variable="author">
      <name form="short" and="text" delimiter=", " initialize-with="."/>
      <label form="short" prefix=", "/>
    </names>
  </macro>

  <macro name="title">
    <text variable="title"/>
  </macro>

  <citation>
    <sort>
      <key variable="citation-number"/>
    </sort>
    <layout delimiter=",">
      <text variable="citation-number" prefix="[" suffix="]"/>
    </layout>
  </citation>

  <bibliography>
    <layout>
      <text variable="citation-number" prefix="[" suffix="] "/>
      <text variable="title"/>
    </layout>
  </bibliography>
</style>
